{
  "env": {
    "jest": true,
    "browser": true,
    "jquery": true,
    "es2020": true,
    "worker": true,
    "serviceworker": true,
    "webextensions": true
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "warn"
  }
}