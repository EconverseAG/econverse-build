{
  "env": {
    "browser": true,
    "jquery": true,
    "es2020": true,
    "worker": true,
    "serviceworker": true,
    "webextensions": true
  },
  "extends": ["eslint:recommended"],
  "parser": "@babel/eslint-parser",
  "rules": {
    "use-isnan": "off",
    "quote-props": "off",
    "no-undef": "off",
    "no-empty": "off",
    "no-redeclare": "off",
    "no-extra-semi": "off",
    "no-cond-assign": "off",
    "no-unused-vars": "off",
    "no-unreachable": "off",
    "no-global-assign": "off",
    "no-useless-escape": "off",
    "no-mixed-spaces-and-tabs": "off",
    "no-extra-boolean-cast": "off",
    "no-case-declarations": "off"
  }
}